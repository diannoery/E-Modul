<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/quiz_tiga"
    app:startDestination="@id/quizMateriTigaFragment">

    <fragment
        android:id="@+id/quizMateriTigaFragment"
        android:name="com.example.mediapembelajaran.ui.quiz.QuizMateriTigaFragment"
        android:label="fragment_quiz_materi_tiga"
        tools:layout="@layout/fragment_quiz_materi_tiga" >
        <action
            android:id="@+id/action_quizMateriTigaFragment_to_resultFragment3"
            app:destination="@id/resultFragment3" />
    </fragment>
    <fragment
        android:id="@+id/resultFragment3"
        android:name="com.example.mediapembelajaran.ui.quiz.ResultFragment"
        android:label="fragment_result"
        tools:layout="@layout/fragment_result" >
        <action
            android:id="@+id/action_resultFragment3_to_menuActivity"
            app:destination="@id/menuActivity" />
    </fragment>
    <activity
        android:id="@+id/menuActivity"
        android:name="com.example.mediapembelajaran.MenuActivity"
        android:label="app_bar_main"
        tools:layout="@layout/app_bar_main" />
</navigation>